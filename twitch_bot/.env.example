# Twitch chat bot credentials (the bot account)
TWITCH_NICK=antigaz_bot
TWITCH_TOKEN=oauth:PASTE_YOUR_TOKEN_HERE

# Which channels the bot is allowed to write to (comma-separated, without #)
TWITCH_CHANNELS=antigaz

# Webhook that the main app will call
BOT_HOST=127.0.0.1
BOT_PORT=5055

# Optional shared secret; set the same value in the main app
TWITCH_BOT_WEBHOOK_SECRET=change_me
